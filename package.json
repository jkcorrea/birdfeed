{
  "name": "birdfeed-ai",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "db:generate-types": "prisma generate",
    "db:prepare-migration": "prisma migrate dev --create-only --skip-seed",
    "db:deploy-migration": "prisma migrate deploy",
    "dev": "run-p dev:*",
    "dev:remix": "remix dev | pino-pretty",
    "dev:stripe": "stripe listen --forward-to localhost:3000/api/webhook/stripe",
    "format": "eslint --fix .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "run-s setup:db setup:stripe setup:seed",
    "setup:db": "prisma generate && prisma migrate deploy",
    "setup:seed": "prisma db seed",
    "setup:stripe": "tsx ./scripts/setup-stripe.ts",
    "start": "remix-serve build",
    "start:ci": "dotenv -- remix-serve build",
    "typecheck": "tsc -b",
    "validate": "run-p lint typecheck",
    "playground": "scripts/playground.sh"
  },
  "dependencies": {
    "@deepgram/sdk": "^1.18.1",
    "@google-cloud/storage": "^6.9.4",
    "@headlessui/react": "^1.7.13",
    "@heroicons/react": "^2.0.16",
    "@paralleldrive/cuid2": "^2.2.0",
    "@prisma/client": "^4.11.0",
    "@remix-run/node": "^1.14.3",
    "@remix-run/react": "^1.14.3",
    "@remix-run/serve": "^1.14.3",
    "@remix-run/server-runtime": "^1.14.3",
    "@supabase/supabase-js": "^2.10.0",
    "array-keyed-map": "^2.1.3",
    "autoprefixer": "^10.4.14",
    "cookie": "^0.5.0",
    "country-to-currency": "^1.0.7",
    "daisyui": "^2.51.4",
    "dayjs": "^1.11.7",
    "eventemitter3": "^5.0.0",
    "framer-motion": "^10.3.2",
    "gpt-3-encoder": "^1.1.4",
    "gpt3-tokenizer": "^1.1.5",
    "huggingface": "^1.4.0",
    "isbinaryfile": "^5.0.0",
    "isbot": "^3.6.7",
    "langchain": "^0.0.36",
    "lodash-es": "^4.17.21",
    "oauth": "^0.10.0",
    "oauth-1.0a": "^2.2.6",
    "openai": "^3.2.1",
    "p-queue": "^7.3.4",
    "p-timeout": "^6.1.1",
    "pino": "^8.11.0",
    "postcss": "^8.4.21",
    "posthog-js": "^1.51.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hot-toast": "^2.4.0",
    "react-wrap-balancer": "^0.4.0",
    "react-zorm": "^0.6.1",
    "remix-utils": "^6.0.0",
    "stripe": "^11.14.0",
    "tailwind-merge": "^1.10.0",
    "zod": "^3.21.4",
    "zodix": "^0.4.0"
  },
  "devDependencies": {
    "@ngneat/falso": "^6.4.0",
    "@remix-run/dev": "^1.14.3",
    "@remix-run/eslint-config": "^1.14.3",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.9",
    "@types/lodash-es": "^4.17.7",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "change-case": "^4.1.2",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.1.0",
    "esbuild": "^0.17.11",
    "esbuild-runner": "^2.2.2",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-tailwindcss": "^3.10.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "fast-glob": "^3.2.12",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^9.4.0",
    "prettier": "2.8.3",
    "prettier-plugin-tailwindcss": "^0.2.4",
    "prisma": "^4.11.0",
    "remix-flat-routes": "^0.5.8",
    "supabase": "^1.42.7",
    "tailwind-scrollbar": "^2.1.0",
    "tailwindcss": "^3.2.7",
    "tsconfig-paths": "^4.1.2",
    "tsx": "^3.12.5",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=18"
  },
  "prisma": {
    "schema": "app/database/schema.prisma",
    "seed": "tsx scripts/seed.ts"
  }
}
